<nav class="breadcrumbs" aria-label="Breadcrumb">
  <ol>
    {% for crumb in breadcrumbs %}
      <li>
        {% if not loop.last %}
          <a href="{{ crumb.url }}">{{ crumb.title }}</a>
        {% else %}
          <span>{{ crumb.title }}</span>
        {% endif %}
      </li>
    {% endfor %}
  </ol>
</nav>
